<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.leandroruiz96.behaviourstests">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".zoomleft.ScrollingActivity"
            android:theme="@style/AppTheme.Collapsing" />
        <activity
            android:name=".snackfab.SnackFabActivity"
            android:label="@string/title_activity_snack_fab"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.github.leandroruiz96.behaviourstests.MainActivity" />
        </activity>
        <activity
            android:name=".tooltipview.TooltipActivity"
            android:label="@string/title_activity_tooltip"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>